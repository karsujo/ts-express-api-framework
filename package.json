    "@types/node": "^20.5.9",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "eslint": "^8.48.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "swagger-jsdoc": "^6.2.8",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
