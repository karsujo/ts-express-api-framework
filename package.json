{
  "name": "ts-express-api-framework",
  "version": "1.0.0",
  "description": "A starter for nodejs-express based api servers",
  "repository": {
    "type": "git",
    "url": "https://github.com/karsujo/ts-express-api-framework"
  },
  "main": "server.ts",
  "scripts": {
    "dev": "npm run build && nodemon ./src/server.ts",
